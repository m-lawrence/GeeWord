<%= form_for @contribution do |f| %>
    <%= f.label :contributor_content %>
    <%= f.text_field :contributor_content %>
    <%= f.label :user_id %>
    <%= f.collection_select :user_id, User.all, :id, :username %>
    <%= f.label :story_id%>
    <%= f.collection_select :story_id, Story.all, :id, :title %>
    <%= f.submit %>
    <% end %>